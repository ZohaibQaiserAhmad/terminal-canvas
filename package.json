{
  "name": "terminal-canvas",
  "version": "3.0.0",
  "description": "Manipulate the cursor in your terminal via high-performant, low-level, canvas-like API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/ghaiklor/terminal-canvas#readme",
  "keywords": [
    "terminal",
    "cursor",
    "canvas",
    "renderer",
    "render"
  ],
  "files": [
    "./dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/ghaiklor/terminal-canvas/issues",
    "email": "ghaiklor@gmail.com"
  },
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "http://ghaiklor.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/terminal-canvas.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "all": "npm run clean && npm run build && npm run lint && npm run test",
    "build": "tsc",
    "clean": "rimraf coverage dist tsconfig.tsbuildinfo",
    "commit": "git cz",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "docs": "typedoc ./src",
    "lint": "eslint --fix --ext .js,.ts .",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run all",
    "preversion": "npm run all",
    "test": "jest",
    "version": "git add ."
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/node": "13.7.4",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-config-standard-with-typescript": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "git-cz": "4.3.1",
    "jest": "25.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "typedoc": "0.16.10",
    "typescript": "3.8.2"
  },
  "peerDependencies": {
    "chop-stream": "0.0.2",
    "speaker": "0.5.1",
    "stream-throttle": "0.1.3",
    "youtube-terminal": "0.8.0",
    "ytdl-core": "2.0.0"
  }
}
